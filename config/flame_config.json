{
    "orangutan": "bin/orng",
    "script_repo": "script/flame_script/",
    "device_name": "flame",
    "resolution": "fwvga",
    "res_x": 480,
    "res_y": 854,
    "event": "/dev/input/event0",
    "execution_time": 1,
    "log_repo_ondevices": "logs",
    "work_dir": "/storage/sdcard0/orangutan/",
    "logs": {
        "logcat": True,
        "b2g-ps": True,
        "b2g-info": True,
        "b2g-procrank": True,
        "dumpstate": True,
        "crash-report": True,
        "period": 60
    }
}
